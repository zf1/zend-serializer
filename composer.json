{
	"name": "zf1/zend-serializer",
	"description": "Zend Framework 1 Serializer package",
	"keywords": [
		"framework",
		"zf1",
		"zend",
		"serializer"
	],
	"homepage": "http://framework.zend.com/",
	"license": "BSD-3-Clause",
	"require": {
		"php": ">=5.2.11",
		"zf1/zend-exception": "self.version",
		"zf1/zend-loader": "self.version",
		"zf1/zend-xml": "self.version"
	},
	"autoload": {
		"psr-0": {
			"Zend_Serializer": "library/"
		}
	},
	"suggest": {
		"zf1/zend-json": "Used in special situations or with special adapters",
		"zf1/zend-amf": "Used in special situations or with special adapters"
	}
}
